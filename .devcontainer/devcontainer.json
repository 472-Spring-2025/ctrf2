{
	"name": "Development",
	"build": {
	  "dockerfile": "../Dockerfile",
	  "context": ".."
	},
	"remoteUser": "root",
	"runArgs": [
	  "--privileged",
	  "-v", "${localWorkspaceFolder}/pwn.college:/opt/pwn.college",
	  "-v", "${localWorkspaceFolder}/data:/data",
	  "-p", "22:22",
	  "-p", "80:80",
	  "-p", "443:443"
	],
	"forwardPorts": [22, 80, 443],
	"customizations": {
	  "vscode": {
		"extensions": [
		  "ms-python.python",
		  "ms-azuretools.vscode-docker",
		  "ms-vscode-remote.remote-containers"
		]
	  }
	}
  }
